{
  "framework": "nextjs",
  "rootDirectory": "packages/ui",
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "outputDirectory": ".next",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
