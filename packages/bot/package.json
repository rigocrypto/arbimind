{
  "name": "@arbimind/bot",
  "version": "1.0.0",
  "description": "ArbiMind arbitrage bot - Off-chain opportunity detection and execution",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "npm run build --workspace=@arbimind/bot || npm run build"
  },
  "deploy": {
    "startCommand": "npm run start --workspace=@arbimind/bot || npm run start"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "canary": "tsx src/canary.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ai": "tsx src/test-ai.ts",
    "predictor": "tsx src/run-predictor.ts",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "axios": "^1.13.5",
    "big.js": "^6.2.1",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "lodash": "^4.17.23",
    "ml-matrix": "^6.10.4",
    "ml-regression": "^5.0.0",
    "natural": "^6.10.4",
    "node-cron": "^3.0.3",
    "sentiment": "^5.0.2",
    "technicalindicators": "^3.1.0",
    "viem": "^2.21.0",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.54.0",
    "happy-dom": "^20.0.2",
    "jsdom": "^24.1.1",
    "msw": "^2.0.20",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "arbitrage",
    "mev",
    "defi",
    "ethereum",
    "bot",
    "trading"
  ],
  "author": "ArbiMind Team",
  "license": "MIT"
}
